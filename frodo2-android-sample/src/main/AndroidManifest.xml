<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.fernandocejas.example.frodo"
    xmlns:android="http://schemas.android.com/apk/res/android">

  <application
      android:allowBackup="true"
      android:icon="@drawable/ic_launcher"
      android:label="@string/app_name"
      android:theme="@style/AppTheme">
    <activity
        android:name="com.fernandocejas.example.frodo.MainActivity"
        android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity
        android:name="com.fernandocejas.example.frodo.SamplesActivity"
        android:label="@string/activity_title_examples" />
  </application>

</manifest>
